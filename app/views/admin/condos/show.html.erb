<h1><%= @condo.address %></h1>
<br>
<% if @condo.users %>
<% @condo.users.each do |user| %>
<h4><p>Tennant: <%= user.name %></p></h4>
<% if user.suite %>
<p>Suite No: <%= user.suite %></p>
<% end %>
<% if user.parking_spot %>
<p>Parking Spot No: <%= user.parking_spot %></p>
<% end %>
<% end %>
<% end %>